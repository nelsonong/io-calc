{
  "name": "ioi-calculators",
  "author": "IO Industries",
  "description": "IOI Calculator",
  "version": "1.1.0",
  "homepage": "./",
  "main": "./public/electron.js",
  "scripts": {
    "build": "webpack -p --env production",
    "dev": "webpack-dev-server",
    "start": "node server/server.js",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=public/electron.js",
    "preelectron-pack": "yarn build",
    "heroku-postbuild": "yarn build"
  },
  "build": {
    "productName": "IOI Calculator",
    "appId": "com.electron.ioi-calculator",
    "win": {
      "icon": "public/ioi-icon.png",
      "target": "nsis"
    },
    "nsis": {
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": true
    },
    "files": [
      "public/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "electron-is-dev": "^1.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "normalize-css": "^2.3.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.2.1",
    "postcss-simple-vars": "^4.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-favicon": "^0.0.17",
    "react-icons": "^2.2.7",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.8.3",
    "redux": "^4.0.0",
    "style-loader": "^0.21.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^13.6.6",
    "electron-builder": "^22.8.0",
    "wait-on": "^5.2.0",
    "webpack-dev-server": "^3.1.4"
  }
}
